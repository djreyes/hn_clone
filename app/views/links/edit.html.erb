<div class="hero-unit"><h1>Edit link</h1></div>
<center>
<%= form_for(@link) do |link|%>

  <%= link.label :title, "Enter Title below"%>
  <%= link.text_field :title%>

  <%= link.label :url, "Enter URL below"%>
  <%= link.text_field :url %>

  <br>
  <%= link.submit "Confirm Change" %>

<% end%>
</center>